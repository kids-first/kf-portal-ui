  "scripts": {
    "start": "npm run theme && cross-env NODE_PATH=\"src\" node scripts/start.js",
    "start-dev": "cross-env NODE_PATH=\"src\" node scripts/start.js --env-file=.env.development.local",
    "build": "cross-env NODE_PATH=\"src\" node scripts/build.js",
    "test": "cross-env NODE_PATH=\"src\" node scripts/test.js --env=jsdom",
    "test:unit": "cross-env NODE_ENV=\"test\" NODE_PATH=\"src\" nyc --reporter=html mocha -w --require babel-core/register src/**/*.unit.js",
    "theme": "lessc --js src/antdTheme.less src/antd-kf-theme.css"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
