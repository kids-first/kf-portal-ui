{
  "name": "kf-portal-ui",
  "version": "2.12.0",
  "private": true,
  "dependencies": {
    "@kfarranger/components": "^1.4.2",
    "@nivo/bar": "^0.51.0",
    "@nivo/core": "^0.51.0",
    "@nivo/pie": "^0.51.0",
    "@oncojs/survivalplot": "^0.7.2",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "antd": "^3.25.3",
    "auto-bind-es5": "^1.1.0",
    "axios": "^0.19.0",
    "file-saver": "^2.0.2",
    "freactal": "^2.0.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "md5": "^2.2.1",
    "memoize-one": "^5.1.1",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.11.0",
    "react-ga": "^2.7.0",
    "react-grid-system": "^6.0.3",
    "react-modal": "^3.10.1",
    "react-places-autocomplete": "^7.2.1",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.3.0",
    "react-share": "^3.0.1",
    "react-sizeme": "^2.6.10",
    "react-slick": "^0.23.2",
    "react-social-icons": "^4.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "scriptjs": "^2.5.9",
    "slick-carousel": "^1.8.1",
    "tinygradient": "^1.0.0",
    "typescript": "^3.6.4",
    "yup": "^0.27.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prepare": "npm run theme",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:unit": "cross-env NODE_ENV=\"test\" NODE_PATH=\"src\" nyc --reporter=html mocha -w --require babel-core/register src/**/*.unit.js",
    "theme": "lessc --js src/antdTheme.less src/antd-kf-theme.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "less": "^3.10.3",
    "source-map-explorer": "^2.1.2"
  }
}
